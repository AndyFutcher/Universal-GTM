{
    "workbench.colorTheme": "Visual Studio Dark"
    ,"workbench.colorCustomizations": {
        "[*Dark*]": {
            "editor.background": "#202428"
            ,"sideBar.background": "#202428"
            ,"minimap.background": "#17181a"
            ,"scrollbarSlider.background": "#00000090"
        }
    },"editor.tokenColorCustomizations": {
    
        "[*Dark*]": {
             "semanticHighlighting": true
            ,"comments": "#616468"
            ,"functions": "#ffffff"
            ,"keywords": "#ffe000"
            ,"variables": "#54b0e8"
            ,"numbers": "#a25049"
            ,"strings": "#9bff3c"
            ,"types": "#FF0000"
        }
    },"editor.semanticTokenColorCustomizations": {
        "[*Dark*]": {
            "enabled": true
            ,"rules": {  // Comments
                 "*.comment": { "bold": false }
                ,"*.functions": { "bold": true }
            }
        }
    },
    "editor.fontFamily": "Menlo, Consolas, DejaVu Sans Mono, monospace",
    "editor.fontSize": 13,
    "debug.console.fontSize": 13,
    "markdown.preview.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "interactiveSession.editor.fontSize": 13
}
